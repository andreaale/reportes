      <Query>
        <DataSourceName>ejemplo</DataSourceName>
        <CommandText>select e.last_name,e.job_id,d.department_id,d.department_name from EMPLOYEES e join DEPARTMENTS d
on (d.DEPARTMENT_ID=e.DEPARTMENT_ID)
join LOCATIONS i on (i.location_id= d.location_id) 
where i.CITY='Toronto' or i.CITY='toronto';</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
